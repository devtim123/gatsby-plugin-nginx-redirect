{
  "name": "gatsby-plugin-nginx-redirect",
  "version": "0.0.6",
  "description": "A minimal boilerplate for the essential files Gatsby looks for in a plugin",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "nginx",
    "redirect"
  ],
  "author": "Guilherme Barbosa Ferreira <gimoteco@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^9.0.0",
    "nginx-conf": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "babel-preset-gatsby-package": "^0.4.2",
    "cross-env": "^5.2.1"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
